{
  "name": "@modyqyw/fabric",
  "version": "5.14.0",
  "description": "Opinionated shareable specification for different JavaScript/TypeScript projects",
  "keywords": [
    "fabric",
    "specification",
    "config",
    "front-end",
    "frontend",
    "prettier",
    "markdownlint",
    "eslint",
    "stylelint",
    "commitlint",
    "husky",
    "lint-staged",
    "javascript",
    "typescript",
    "js",
    "ts",
    "react",
    "react-native",
    "taro",
    "rax",
    "remax",
    "vue",
    "vue2",
    "vue3",
    "uniapp",
    "uni-app",
    "svelte",
    "css",
    "less",
    "scss",
    "git",
    "naming"
  ],
  "homepage": "https://github.com/ModyQyW/fabric#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/fabric/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/fabric.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./eslint": {
      "types": "./dist/eslint/index.d.ts",
      "import": "./dist/eslint/index.mjs",
      "require": "./dist/eslint/index.js"
    },
    "./eslint/*": {
      "types": "./dist/eslint/*.d.ts",
      "import": "./dist/eslint/*.mjs",
      "require": "./dist/eslint/*.js"
    },
    "./prettier": {
      "types": "./dist/prettier/index.d.ts",
      "import": "./dist/prettier/index.mjs",
      "require": "./dist/prettier/index.js"
    },
    "./stylelint": {
      "types": "./dist/stylelint/index.d.ts",
      "import": "./dist/stylelint/index.mjs",
      "require": "./dist/stylelint/index.js"
    },
    "./stylelint/*": {
      "types": "./dist/stylelint/*.d.ts",
      "import": "./dist/stylelint/*.mjs",
      "require": "./dist/stylelint/*.js"
    },
    "./package.json": "./package.json",
    "./tsconfig.base.json": "./tsconfig.base.json"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "tsconfig.base.json"
  ],
  "scripts": {
    "build": "tsup-node",
    "commit": "git-cz",
    "check:types": "tsc --noEmit",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue,.svelte --ignore-path=.gitignore --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it"
  },
  "dependencies": {
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/prompt": "^17.3.0",
    "@commitlint/types": "^17.0.0",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/eslint": "^8.4.10",
    "@types/node": "latest",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-css": "^0.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jsonc": "^2.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-regexp": "^1.11.0",
    "eslint-plugin-solid": "^0.9.1",
    "eslint-plugin-sonarjs": "^0.17.0",
    "eslint-plugin-svelte": "^2.14.1",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-vue": "^9.8.0",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "eslint-plugin-vuejs-accessibility": "^2.0.0",
    "eslint-plugin-yml": "^1.4.0",
    "jsonc-eslint-parser": "^2.1.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-packagejson": "^2.3.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "svelte-eslint-parser": "^0.22.3",
    "ts-node": "^10.9.1",
    "tsutils": "^3.21.0",
    "typescript": "^4.9.4",
    "vue-eslint-parser": "^9.1.0",
    "yaml-eslint-parser": "^1.1.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "commitizen": "^4.2.6",
    "eslint": "^8.31.0",
    "eslint-define-config": "^1.13.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.32.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "release-it": "^15.6.0",
    "sort-package-json": "^2.1.0",
    "stylelint": "^14.16.1",
    "tsup": "^6.5.0",
    "type-fest": "^3.5.0"
  },
  "packageManager": "pnpm@7.21.0",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
