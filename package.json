{
  "name": "@modyqyw/fabric",
  "version": "7.4.0",
  "description": "Opinionated shareable specification for different JavaScript/TypeScript projects",
  "keywords": [
    "fabric",
    "specification",
    "config",
    "front-end",
    "frontend",
    "prettier",
    "markdownlint",
    "eslint",
    "stylelint",
    "commitlint",
    "husky",
    "lint-staged",
    "javascript",
    "typescript",
    "js",
    "ts",
    "react",
    "rn",
    "react-native",
    "taro",
    "vue",
    "vue2",
    "vue3",
    "uniapp",
    "uni-app",
    "css",
    "scss",
    "git",
    "naming"
  ],
  "homepage": "https://github.com/ModyQyW/fabric#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/fabric/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/fabric.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./eslint": {
      "types": "./dist/eslint.d.ts",
      "import": "./dist/eslint.mjs",
      "require": "./dist/eslint.js"
    },
    "./prettier": {
      "types": "./dist/prettier.d.ts",
      "import": "./dist/prettier.mjs",
      "require": "./dist/prettier.js"
    },
    "./stylelint": {
      "types": "./dist/stylelint.d.ts",
      "import": "./dist/stylelint.mjs",
      "require": "./dist/stylelint.js"
    },
    "./package.json": "./package.json",
    "./tsconfig.base.json": "./tsconfig.base.json"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "tsconfig.base.json"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -u -r 0 && git add CHANGELOG.md",
    "commit": "git-cz",
    "dev": "pnpm run build --watch",
    "lint": "run-p lint:eslint lint:markdownlint lint:publint check:types",
    "lint:eslint": "eslint . --fix --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore=CHANGELOG.md --ignore-path=.gitignore",
    "lint:publint": "publint",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm install && pnpm run build && pnpm run lint && bumpp --all -x \"pnpm run changelog\""
  },
  "dependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt": "^17.4.4",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/eslint": "^8.21.1",
    "@types/node": "latest",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@unocss/eslint-config": "^0.50.4",
    "@unocss/eslint-plugin": "^0.50.4",
    "debug": "^4.3.4",
    "eslint-config-next": "^13.2.3",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "eslint-plugin-regexp": "^1.12.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vue-scoped-css": "^2.4.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint-plugin-yml": "^1.5.0",
    "jsonc-eslint-parser": "^2.1.0",
    "local-pkg": "^0.4.3",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-order": "^6.0.3",
    "tsutils": "^3.21.0",
    "typescript": "^4.9.5",
    "vue-eslint-parser": "^9.1.0",
    "yaml-eslint-parser": "^1.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/debug": "^4.1.7",
    "@types/shelljs": "^0.8.11",
    "bumpp": "^9.0.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "esbuild": "^0.17.11",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.2",
    "markdownlint-cli": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "publint": "^0.1.10",
    "rollup": "^3.18.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "shelljs": "^0.8.5",
    "sort-package-json": "^2.4.1",
    "stylelint": "^15.2.0",
    "type-fest": "^3.6.1"
  },
  "peerDependencies": {
    "@types/eslint": "^8.0.0",
    "@types/prettier": "^2.0.0",
    "eslint": "^8.0.0",
    "postcss": "^8.0.0",
    "prettier": "^2.0.0",
    "react": "^18.0.0",
    "stylelint": "^15.0.0",
    "vue": "^3.2.0 || ^2.7.0 || ^2.6.0"
  },
  "peerDependenciesMeta": {
    "@types/eslint": {
      "optional": true
    },
    "@types/prettier": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "postcss": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "stylelint": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "packageManager": "pnpm@7.29.0",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
