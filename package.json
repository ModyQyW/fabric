{
  "name": "@modyqyw/fabric",
  "version": "3.14.0",
  "description": "Opinionated shareable specification for different JavaScript/TypeScript projects.",
  "keywords": [
    "fabric",
    "specification",
    "config",
    "front-end",
    "frontend",
    "prettier",
    "markdownlint",
    "eslint",
    "stylelint",
    "commitlint",
    "husky",
    "lint-staged",
    "javascript",
    "typescript",
    "js",
    "ts",
    "react",
    "react-native",
    "taro",
    "rax",
    "remax",
    "vue",
    "vue2",
    "vue3",
    "uniapp",
    "uni-app",
    "css",
    "less",
    "scss",
    "sass",
    "git",
    "naming"
  ],
  "homepage": "https://github.com/ModyQyW/fabric#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/fabric/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/fabric.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui7@mail3.sysu.edu.cn",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs.js",
  "jsdelivr": "./dist/index.cjs.js",
  "unpkg": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mo-fabric": "./cli.js",
    "modyqyw-fabric": "./cli.js"
  },
  "files": [
    "dist",
    "examples",
    ".editorconfig",
    ".gitattributes",
    ".gitignore",
    ".markdownlint.json",
    "cli.js",
    "tsconfig.base.json"
  ],
  "scripts": {
    "build": "tsc && vite build && rollup -i ./src/index.ts -o ./dist/index.d.ts -f es -p dts",
    "dev": "vite",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && sort-package-json",
    "lint:eslint": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it"
  },
  "lint-staged": {
    "*.md": "markdownlint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/core": "~7.16.0",
    "@babel/eslint-parser": "~7.16.3",
    "@commitlint/config-conventional": "~15.0.0",
    "@commitlint/types": "~15.0.0",
    "@types/eslint": "~7.29.0",
    "@types/prettier": "~2.4.2",
    "@types/stylelint": "~13.13.3",
    "@typescript-eslint/eslint-plugin": "~5.5.0",
    "@typescript-eslint/parser": "~5.5.0",
    "commander": "~8.3.0",
    "eslint-config-airbnb": "~19.0.2",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-airbnb-typescript": "~16.1.0",
    "eslint-config-next": "~12.0.5",
    "eslint-config-prettier": "~8.3.0",
    "eslint-import-resolver-node": "~0.3.6",
    "eslint-import-resolver-typescript": "~2.5.0",
    "eslint-import-resolver-webpack": "~0.13.2",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.25.3",
    "eslint-plugin-jsx-a11y": "~6.5.1",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-react": "~7.27.1",
    "eslint-plugin-react-hooks": "~4.3.0",
    "eslint-plugin-regexp": "~1.5.1",
    "eslint-plugin-unicorn": "~39.0.0",
    "eslint-plugin-vue": "~8.1.1",
    "eslint-plugin-vuejs-accessibility": "~1.1.0",
    "inquirer": "~8.2.0",
    "picocolors": "~1.0.0",
    "postcss": "~7.0.39",
    "shelljs": "~0.8.4",
    "stylelint-config-prettier": "~9.0.3",
    "stylelint-config-recess-order": "~2.6.0",
    "stylelint-config-standard": "~22.0.0",
    "stylelint-config-standard-scss": "~1.1.0",
    "stylelint-prettier": "~1.2.0",
    "stylelint-scss": "~3.21.0",
    "typescript": "~4.5.2",
    "update-notifier": "~5.1.0",
    "vue-class-component": "~7.2.6",
    "vue-eslint-parser": "~8.0.1",
    "vue-property-decorator": "~9.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "~15.0.0",
    "@types/inquirer": "~8.1.3",
    "@types/shelljs": "~0.8.9",
    "@types/update-notifier": "~5.1.0",
    "cnpm": "~7.1.0",
    "commitizen": "~4.2.4",
    "eslint": "~7.32.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.1.2",
    "markdownlint-cli": "~0.30.0",
    "prettier": "~2.5.0",
    "release-it": "~14.11.8",
    "rollup": "~2.60.2",
    "rollup-plugin-dts": "~4.0.1",
    "sort-package-json": "~1.53.1",
    "stylelint": "~13.13.1",
    "vite": "~2.6.14"
  },
  "engines": {
    "node": "^12.22.6 || ^14.17.6 || ^16.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
