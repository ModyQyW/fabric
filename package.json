{
  "name": "@modyqyw/fabric",
  "version": "6.0.5",
  "description": "Opinionated shareable specification for different JavaScript/TypeScript projects",
  "keywords": [
    "fabric",
    "specification",
    "config",
    "front-end",
    "frontend",
    "prettier",
    "markdownlint",
    "eslint",
    "stylelint",
    "commitlint",
    "husky",
    "lint-staged",
    "javascript",
    "typescript",
    "js",
    "ts",
    "react",
    "rn",
    "react-native",
    "taro",
    "vue",
    "vue2",
    "vue3",
    "uniapp",
    "uni-app",
    "css",
    "scss",
    "git",
    "naming"
  ],
  "homepage": "https://github.com/ModyQyW/fabric#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/fabric/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/fabric.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./eslint": {
      "types": "./dist/eslint.d.ts",
      "import": "./dist/eslint.mjs",
      "require": "./dist/eslint.js"
    },
    "./prettier": {
      "types": "./dist/prettier.d.ts",
      "import": "./dist/prettier.mjs",
      "require": "./dist/prettier.js"
    },
    "./stylelint": {
      "types": "./dist/stylelint.d.ts",
      "import": "./dist/stylelint.mjs",
      "require": "./dist/stylelint.js"
    },
    "./package.json": "./package.json",
    "./tsconfig.base.json": "./tsconfig.base.json"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "tsconfig.base.json"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "pnpm run build --watch",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --fix --cache --no-error-on-unmatched-pattern",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pnpm run build",
    "release": "release-it"
  },
  "dependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/prompt": "^17.4.2",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/eslint": "^8.4.10",
    "@types/node": "latest",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "eslint-plugin-regexp": "^1.12.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vue-scoped-css": "^2.4.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint-plugin-yml": "^1.4.0",
    "jsonc-eslint-parser": "^2.1.0",
    "local-pkg": "^0.4.3",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-packagejson": "^2.4.2",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^6.0.1",
    "stylelint-prettier": "^2.0.0",
    "tsutils": "^3.21.0",
    "typescript": "^4.9.5",
    "vue-eslint-parser": "^9.1.0",
    "yaml-eslint-parser": "^1.1.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/shelljs": "^0.8.11",
    "commitizen": "^4.3.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "release-it": "^15.6.0",
    "rollup": "^3.12.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "shelljs": "^0.8.5",
    "sort-package-json": "^2.4.0",
    "stylelint": "^14.16.1",
    "type-fest": "^3.5.4"
  },
  "peerDependencies": {
    "@types/eslint": "^8.0.0",
    "@types/prettier": "^2.0.0",
    "eslint": "^8.0.0",
    "postcss": "^8.0.0",
    "prettier": "^2.0.0",
    "react": "^18.0.0",
    "stylelint": "^14.16.1",
    "vue": "^3.2.0 || ^2.7.0 || ^2.6.0"
  },
  "peerDependenciesMeta": {
    "@types/eslint": {
      "optional": true
    },
    "@types/prettier": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "postcss": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "stylelint": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "packageManager": "pnpm@7.26.3",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
