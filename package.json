{
  "name": "@modyqyw/fabric",
  "version": "4.13.1",
  "description": "Opinionated shareable specification for different JavaScript/TypeScript projects.",
  "keywords": [
    "fabric",
    "specification",
    "config",
    "front-end",
    "frontend",
    "prettier",
    "markdownlint",
    "eslint",
    "stylelint",
    "commitlint",
    "husky",
    "lint-staged",
    "javascript",
    "typescript",
    "js",
    "ts",
    "react",
    "react-native",
    "taro",
    "rax",
    "remax",
    "vue",
    "vue2",
    "vue3",
    "uniapp",
    "uni-app",
    "svelte",
    "css",
    "less",
    "scss",
    "sass",
    "git",
    "naming"
  ],
  "homepage": "https://github.com/ModyQyW/fabric#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/fabric/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/fabric.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui7@mail3.sysu.edu.cn",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./tsconfig.base.json": "./tsconfig.base.json"
  },
  "main": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "unpkg": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "mo-fabric": "./dist/cli.js"
  },
  "files": [
    "dist",
    "tsconfig.base.json",
    "package.json"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsup-node src/index.ts --dts --format esm,cjs --minify && tsup-node src/cli.ts --minify",
    "commit": "git-cz",
    "dev": "vite",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && tsc --noEmit && sort-package-json",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue,.svelte --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "dependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt": "^16.2.1",
    "@commitlint/types": "^16.2.1",
    "@ota-meshi/eslint-plugin-svelte": "^0.29.0",
    "@types/eslint": "^8.4.1",
    "@types/node": "latest",
    "@types/prettier": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "commander": "^9.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-unicorn": "^41.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "inquirer": "^8.2.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.3",
    "prettier-plugin-jsdoc": "^0.3.30",
    "prettier-plugin-packagejson": "^2.2.15",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "shelljs": "^0.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "svelte-eslint-parser": "^0.15.0",
    "tsutils": "^3.21.0",
    "typescript": "^4.6.2",
    "update-notifier": "^5.1.0",
    "vue-class-component": "^7.2.6",
    "vue-eslint-parser": "^8.3.0",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@types/inquirer": "^8.2.0",
    "@types/shelljs": "^0.8.11",
    "@types/update-notifier": "^5.1.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.3.4",
    "markdownlint-cli": "^0.31.1",
    "prettier": "^2.5.1",
    "release-it": "^14.12.5",
    "sort-package-json": "^1.54.0",
    "stylelint": "^14.5.3",
    "tsup": "^5.11.13"
  },
  "engines": {
    "node": "^12.22.6 || ^14.17.6 || ^16.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
