{
  "name": "@modyqyw/fabric",
  "version": "2.9.0-0",
  "description": "Opinionated shareable specification for different JavaScript/TypeScript projects.",
  "keywords": [
    "fabric",
    "specification",
    "config",
    "front-end",
    "frontend",
    "prettier",
    "markdownlint",
    "eslint",
    "stylelint",
    "commitlint",
    "ls-lint",
    "husky",
    "lint-staged",
    "javascript",
    "typescript",
    "js",
    "ts",
    "react",
    "react-native",
    "taro",
    "rax",
    "remax",
    "vue",
    "vue2",
    "vue3",
    "uniapp",
    "uni-app",
    "css",
    "less",
    "sass",
    "scss",
    "git",
    "naming"
  ],
  "homepage": "https://github.com/ModyQyW/fabric#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/fabric/issues"
  },
  "bin": {
    "modyqyw-fabric": "./cli.js"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui7@mail3.sysu.edu.cn",
    "url": "https://modyqyw.github.io"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/fabric.git"
  },
  "dependencies": {
    "@babel/core": "~7.14.8",
    "@babel/eslint-parser": "~7.14.7",
    "@commitlint/config-conventional": "~13.1.0",
    "@typescript-eslint/eslint-plugin": "~4.28.5",
    "@typescript-eslint/parser": "~4.28.5",
    "chalk": "~4.1.1",
    "commander": "~8.0.0",
    "eslint-config-airbnb": "~18.2.1",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-config-airbnb-typescript": "~12.3.1",
    "eslint-config-next": "~11.0.1",
    "eslint-config-prettier": "~8.3.0",
    "eslint-import-resolver-node": "~0.3.4",
    "eslint-import-resolver-typescript": "~2.4.0",
    "eslint-import-resolver-webpack": "~0.13.1",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jsx-a11y": "~6.4.1",
    "eslint-plugin-prettier": "~3.4.0",
    "eslint-plugin-react": "~7.24.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-plugin-unicorn": "~34.0.1",
    "eslint-plugin-vue": "~7.14.0",
    "eslint-plugin-vuejs-accessibility": "~0.7.1",
    "inquirer": "~8.1.2",
    "shelljs": "~0.8.4",
    "stylelint-config-prettier": "~8.0.2",
    "stylelint-config-twbs-bootstrap": "~2.2.3",
    "stylelint-prettier": "~1.2.0",
    "typescript": "~4.3.5",
    "vue-class-component": "~7.2.6",
    "vue-eslint-parser": "~7.9.0",
    "vue-property-decorator": "~9.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "~13.1.0",
    "@lint-md/cli": "~0.1.5",
    "@ls-lint/ls-lint": "~1.10.0",
    "commitizen": "~4.2.4",
    "eslint": "~7.31.0",
    "husky": "~7.0.1",
    "is-ci": "~3.0.0",
    "lint-staged": "~11.1.1",
    "markdownlint-cli": "~0.28.1",
    "prettier": "~2.3.2",
    "release-it": "~14.10.1",
    "stylelint": "~13.13.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "eslintConfig": {
    "extends": "./eslint/native"
  },
  "prettier": "./prettier",
  "commitlint": {
    "extends": "./commitlint"
  },
  "lint-staged": {
    "*.json": "prettier --write",
    "*.{md,markdown}": "markdownlint --fix",
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "yarn lint:json && yarn lint:markdown && yarn lint:script",
    "lint:json": "prettier ./**/*.json --write --ignore-path=.gitignore",
    "lint:markdown": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:script": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it"
  }
}
