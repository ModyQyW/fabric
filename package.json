{
  "name": "@modyqyw/fabric",
  "version": "9.0.6",
  "description": "Opinionated shareable specification for different JavaScript/TypeScript projects",
  "keywords": [
    "fabric",
    "specification",
    "config",
    "front-end",
    "frontend",
    "prettier",
    "markdownlint",
    "eslint",
    "stylelint",
    "commitlint",
    "husky",
    "simple-git-hooks",
    "lint-staged",
    "javascript",
    "typescript",
    "js",
    "ts",
    "react",
    "rn",
    "react-native",
    "taro",
    "vue",
    "vue2",
    "vue3",
    "uniapp",
    "uni-app",
    "css",
    "scss",
    "git",
    "naming"
  ],
  "homepage": "https://github.com/ModyQyW/fabric#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/fabric/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/fabric.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./eslint": {
      "types": {
        "import": "./dist/eslint.d.mts",
        "require": "./dist/eslint.d.cts"
      },
      "import": "./dist/eslint.mjs",
      "require": "./dist/eslint.cjs"
    },
    "./prettier": {
      "types": {
        "import": "./dist/prettier.d.mts",
        "require": "./dist/prettier.d.cts"
      },
      "import": "./dist/prettier.mjs",
      "require": "./dist/prettier.cjs"
    },
    "./stylelint": {
      "types": {
        "import": "./dist/stylelint.d.mts",
        "require": "./dist/stylelint.d.cts"
      },
      "import": "./dist/stylelint.mjs",
      "require": "./dist/stylelint.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "check:deps": "taze -f",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "unbuild --stub",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "format": "prettier . \"!*lock*\" --ignore-unknown --write --cache --log-level=warn",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\" \"pnpm:lint:publint\"",
    "lint:eslint": "eslint . --fix --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:publint": "publint",
    "prepare": "is-ci || simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "release": "changelogen --release"
  },
  "dependencies": {
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-angular": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/config-lerna-scopes": "^18.4.3",
    "@commitlint/config-nx-scopes": "^18.4.3",
    "@commitlint/config-pnpm-scopes": "^18.4.3",
    "@commitlint/config-rush-scopes": "^18.4.3",
    "@commitlint/prompt": "^18.4.3",
    "@commitlint/types": "^18.4.3",
    "@eslint/js": "^8.55.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@next/eslint-plugin-next": "^14.0.4",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint": "^8.44.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@unocss/eslint-config": "^0.58.0",
    "@unocss/eslint-plugin": "^0.58.0",
    "eslint-config-flat-gitignore": "^0.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-regexp": "^2.1.2",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-vue": "^9.19.2",
    "eslint-plugin-vue-scoped-css": "^2.5.1",
    "eslint-plugin-yml": "^1.10.0",
    "globals": "^13.24.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier-plugin-css-order": "^2.0.1",
    "prettier-plugin-jsdoc": "^1.1.1",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.7",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "tsutils": "^3.21.0",
    "typescript": "^5.3.3",
    "vue-eslint-parser": "^9.3.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.38.5",
    "@tsconfig/node20": "^20.1.2",
    "@types/micromatch": "^4.0.6",
    "@types/node": "^20.10.4",
    "changelogen": "^0.5.5",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.55.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.0",
    "markdownlint-cli": "^0.38.0",
    "prettier": "^3.1.1",
    "publint": "^0.2.6",
    "react": "^18.2.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.0.1",
    "taze": "^0.13.0",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.0",
    "viewerjs": "^1.11.6",
    "vitepress": "^1.0.0-rc.31",
    "vitepress-plugin-comment-with-giscus": "^1.1.11",
    "vitepress-plugin-image-viewer": "^1.1.2",
    "vue": "^3.3.11"
  },
  "peerDependencies": {
    "@types/eslint": "^8.0.0",
    "eslint": "^8.0.0",
    "postcss": "^8.0.0",
    "prettier": "^3.0.0",
    "react": "^18.0.0",
    "stylelint": "^15.10.0",
    "typescript": "^5.0.0",
    "vue": "^3.2.0 || ^2.7.0 || ^2.6.0"
  },
  "peerDependenciesMeta": {
    "@types/eslint": {
      "optional": true
    },
