    "react": "^18.0.0",
    "stylelint": "^15.10.0",
    "svelte": "^3.37.0 || ^4.0.0",
    "typescript": "^5.0.0",
    "vue": "^3.2.0 || ^2.7.0 || ^2.6.0"
  },
  "peerDependenciesMeta": {
