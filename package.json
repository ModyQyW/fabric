{
  "name": "@modyqyw/fabric",
  "version": "9.0.2",
  "description": "Opinionated shareable specification for different JavaScript/TypeScript projects",
  "keywords": [
    "fabric",
    "specification",
    "config",
    "front-end",
    "frontend",
    "prettier",
    "markdownlint",
    "eslint",
    "stylelint",
    "commitlint",
    "husky",
    "simple-git-hooks",
    "lint-staged",
    "javascript",
    "typescript",
    "js",
    "ts",
    "react",
    "rn",
    "react-native",
    "taro",
    "vue",
    "vue2",
    "vue3",
    "uniapp",
    "uni-app",
    "css",
    "scss",
    "git",
    "naming"
  ],
  "homepage": "https://github.com/ModyQyW/fabric#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/fabric/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/fabric.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./eslint": {
      "types": "./dist/eslint.d.ts",
      "import": "./dist/eslint.mjs",
      "require": "./dist/eslint.cjs"
    },
    "./prettier": {
      "types": "./dist/prettier.d.ts",
      "import": "./dist/prettier.mjs",
      "require": "./dist/prettier.cjs"
    },
    "./stylelint": {
      "types": "./dist/stylelint.d.ts",
      "import": "./dist/stylelint.mjs",
      "require": "./dist/stylelint.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "check:deps": "taze -f",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "unbuild --stub",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\" \"pnpm:lint:publint\"",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:publint": "publint",
    "prepare": "is-ci || simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "release": "commit-and-tag-version -a"
  },
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@commitlint/cli": "^17.7.1",
